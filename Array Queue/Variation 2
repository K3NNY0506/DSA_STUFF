#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>

#define MAX 10

typedef struct {
    int items[MAX];
    int front;
    int rear;
} Queue;

// Initialize queue
Queue* initialize() {
    Queue* q = (Queue*) malloc(sizeof(Queue));
    if (!q) {
        printf("Memory allocation failed!\n");
        exit(1);
    }
    q->front = 1;
    q->rear = 0;
    return q;
}

// Check if empty
bool isEmpty(Queue* q) {
    return q->front == (q->rear + 1) % MAX;
}

// Check if full
bool isFull(Queue* q) {
    return q->front == (q->rear + 2) % MAX;
}

// Enqueue operation
void enqueue(Queue* q, int value) {
    if (isFull(q)) {
        printf("Queue is FULL! Cannot enqueue %d\n", value);
        return;
    }
    q->rear = (q->rear + 1) % MAX;
    q->items[q->rear] = value;
    printf("Enqueued: %d\n", value);
}

// Dequeue operation
int dequeue(Queue* q) {
    if (isEmpty(q)) {
        printf("Queue is EMPTY! Cannot dequeue.\n");
        return -1;
    }
    int value = q->items[q->front];
    q->front = (q->front + 1) % MAX;
    printf("Dequeued: %d\n", value);
    return value;
}

// Get front element
int frontElement(Queue* q) {
    if (isEmpty(q)) {
        printf("Queue is EMPTY! No front element.\n");
        return -1;
    }
    return q->items[q->front];
}

// Display queue
void display(Queue* q) {
    if (isEmpty(q)) {
        printf("Queue is EMPTY!\n");
        return;
    }
    printf("Queue contents: ");
    int i = q->front;
    while (i != (q->rear + 1) % MAX) {
        printf("%d ", q->items[i]);
        i = (i + 1) % MAX;
    }
    printf("\n");
}

// Example main
int main() {
    Queue* q = initialize();

    enqueue(q, 10);
    enqueue(q, 20);
    enqueue(q, 30);
    enqueue(q, 40);

    display(q);

    dequeue(q);
    display(q);

    enqueue(q, 50);
    enqueue(q, 60);
    display(q);

    printf("Front element: %d\n", frontElement(q));

    return 0;
}
